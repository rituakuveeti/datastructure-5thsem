// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int a[]={10,-1,-9,13,17,21,-6,-23,51,33};
    int j,i,temp;
    for(i=0;i<10;i++){
        for(j=0;j<10-i;j++){
            if(a[j]>a[j+1]){
                temp=a[j];
                a[j]=a[j+1];
                a[j+1]=temp;
            }
        }
    }
printf("sorted array is:");
    for(i=0;i<10;i++){
        printf("%d \t",a[i]);
    }
    
}
